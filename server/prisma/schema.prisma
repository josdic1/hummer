generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Idea {
  id          String   @id @default(cuid())
  title       String
  bpm         String
  lyrics      String
  status      String
  created_on  DateTime
  modified_on DateTime
  link        String
}